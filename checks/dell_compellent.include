def dell_compellent_translate(table, entry):
    sc_status = {
        "1": (0, "UP"),
        "2": (2, "DOWN"),
        "3": (1, "DEGRADED"),
    }

    health = {
        "1": 0,
        "0": 2,
    }

    if table == "ScStatus":
        return sc_status[entry]
    elif table == "health":
        return health[entry]


def inventory_dell_compellent(info):
    for line in info:
        yield (line[0], None)
